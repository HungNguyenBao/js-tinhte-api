  },
  "dependencies": {
    "isomorphic-unfetch": "^2.0.0",
    "js-cookie": "^2.2.0",
    "md5": "^2.2.1",
    "prop-types": "^15.6.0",
    "querystring": "^0.2.0"
  },
  "peerDependencies": {
    "babel-runtime": "6.x",
    "react": "16.x"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "nwb": "0.21.x",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-tree-walker": "^2.1.3",
    "standard": "^11.0.0",
    "surge": "^0.19.0"
  },
  "author": "Dao Hoang Son",
  "homepage": "https://tinhte.vn",
